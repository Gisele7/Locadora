@{
    ViewData["Title"] = "Login";
}
<h1>Login</h1>

<form asp-controller="Login" asp-action"Login" method="post">

    <label>Nome usuário:</label>
    <input type="text" name="loginUsuario"/> 
    
    <label>Senha:</label>
    <input type="password" name="senhaUsuario"/>


    <input type="submit" value="logar"/>


</form>
